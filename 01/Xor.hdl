/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
  Nand(a=a, b=a, out=nanda);
  Nand(a=nanda, b=b, out=c);

  Nand(a=b, b=b, out=nandb);
  Nand(a=a, b=nandb, out=d);

  Nand(a=c, b=d, out=out);
}