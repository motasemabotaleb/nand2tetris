/**
 * 16-bit multiplexor: 
 * for i = 0..15 out[i] = a[i] if sel == 0 
 *                        b[i] if sel == 1
 */

CHIP Mux16 {
    IN a[16], b[16], sel;
    OUT out[16];

    PARTS:
    // Put your code here:
  For(int i=0;i=15;i++){
  system.out.println(mux(a=a[''+i+''],b=b[''+i+''],out=out[''+i+''])}
}
